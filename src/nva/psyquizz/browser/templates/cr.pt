<h1> Resultate </h1>

  <ul tal:define="crits view.getCriterias()">
    <li tal:repeat="crit crits">
      <span tal:content="crit"/>
        <ul tal:repeat="crit_item crits[crit]">
          <li tal:content="string: ${crit_item.name} (${crit_item.amount})"/>
        </ul>
    </li>
  </ul>

  <div id="chart">
    <embed type="image/svg+xml" tal:attributes="src view.getRadar()"/>
  </div>

<table>
 <tr tal:repeat="data view.getPData()">
   <td tal:content="data.name"></td> 
   <td tal:content="data.value"></td> 
 </tr>

</table>
