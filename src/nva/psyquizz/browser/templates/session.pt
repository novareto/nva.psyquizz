<div tal:define="baseurl view.url(context)">
  <p>
    Die Erhebung beginnt vom <span tal:content="context.startdate" />
    bis <span tal:content="context.enddate" />.
  </p>
  <dl>
    <dt>Abgeschlossene Fragebogen</dt>
    <dd>
      <ul>
	<li tal:repeat="student context.complete">
	  <a tal:attributes="href '%s/%s' % (baseurl, student.access)"
	      tal:content="student.access" />
	</li>
      </ul>
    </dd>
  </dl>
  <dl>
    <dt>Ausstehende Fragebogen</dt>
    <dd>
      <ul>
	<li tal:repeat="student context.uncomplete">
	  <a tal:attributes="href '%s/%s' % (baseurl, student.access)"
	      tal:content="student.access" />
	</li>
      </ul>
    </dd>
  </dl>
</div>
